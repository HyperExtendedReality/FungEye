{
  "name": "FungEye",
  "slug": "fungeye",
  "android": {
    "package": "com.hyperxr.fungeye"
  },
  "plugins": [
    [
      "react-native-fast-tflite",
      {
        "enableAndroidGpuLibraries": true
      }
    ],
    [
      "react-native-vision-camera",
      {
        "cameraPermissionText": "$(PRODUCT_NAME) needs access to your camera to identify mushrooms.",
        "enableMicrophonePermission": false
      }
    ],
    "expo-sqlite"
  ]
}
